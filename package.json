{
  "name": "ParkingInterconnect",
  "version": "1.0.0",
  "main": "index.js",
  "author": "nkbtemmy <nkubito@augmented-future.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "node build/",
    "build": "rm -rf build && tsc -p .",
    "clean": "rimraf lib",
    "prebuild": "yarn clean",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:unseed": "sequelize db:seed:undo:all",
    "db:undo": "sequelize db:migrate:undo:all",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "db:create": "sequelize db:create",
    "db:drop": "sequelize db:drop",
    "docker": "docker-compose up -d && docker-compose up --build"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.19.1",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.2",
    "express-fileupload": "^1.4.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.5",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1",
    "socket.io": "^4.5.3",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  }
}
